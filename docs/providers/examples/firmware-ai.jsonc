      "name": "Firmware AI",
      "options": {
        "baseURL": "https://app.firmware.ai/api/v1",
        "apiKey": "YOUR_FIRMWARE_API_KEY",
        "timeout": 300000,
        "headers": {
          "Content-Type": "application/json"
